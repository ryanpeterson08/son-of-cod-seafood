@model IEnumerable<SonOfCod.Models.Signup>

<h2>Here are the current Newsletter Recepients</h2>

@if(Model.Any())
{
    @foreach(var signUpItem in Model)
    {
        <ul>
            <li>@Html.DisplayFor(modelItem => signUpItem.userEmail)</li>
            <li>@Html.DisplayFor(modelItem => signUpItem.userAddress)</li>
        </ul>
    }
}
else
{
    <p>There is no one currently signed up for the newsletter :(</p>

}

